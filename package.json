    "webpack-dev-server": "^3.1.8"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/immfly/flights-map.git"
  },
  "keywords": [
    "planes",
    "map",
    "radar",
    "flights"
  ],
  "bugs": {
    "url": "https://github.com/immfly/flights-map/issues"
  },
  "homepage": "https://github.com/immfly/flights-map#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0"
  }
